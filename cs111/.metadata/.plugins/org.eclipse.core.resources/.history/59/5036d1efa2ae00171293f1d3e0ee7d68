
public class Terminal {
	
	public static void main(String[] args) {
		System.out.println("Enter num of test: ");
		int num = IO.readInt();
		int i=0;
		int sum =0;
		while (i<num) {
			System.out.println("Enter test score: ");
			double score = IO.readDouble();
			sum+=score;
		}
		IO.outputDoubleAnswer(sum/num);
	}

}
