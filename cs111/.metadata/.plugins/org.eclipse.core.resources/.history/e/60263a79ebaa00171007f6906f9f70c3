
public class Terminal {
	
	public static void main(String[] args) {
		
		
		System.out.println("Enter age: ");
		int age = IO.readInt();
		if (age >= 18) {
			IO.outputStringAnswer("Can Vote");
		} else {
			IO.outputStringAnswer("Can't Vote");
		}
		
		
		// Q1 Pt B
		double speed, fine;
		String str;
		System.out.println("Enter speed: ");
		speed = IO.readDouble();
		if (speed <=60) {
			fine = 0;
			str = "Fine is "+fine;
			IO.outputStringAnswer(str);
		} else if (speed > 60 && speed <=75 ) {
			fine = 100;
			IO.outputDoubleAnswer(fine);
		} else if (speed > 75 && speed <= 90 ) {
			fine = 200;
			IO.outputDoubleAnswer(fine);
		} else if (speed > 90) {
			fine = 500;
			IO.outputDoubleAnswer(fine);
		}
		
		
		//Q2 part A
		
	}

}
